   "source": [
    "15.SQL Quary to get the nth highest salary from Employee table "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d48b99dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "SELECT DISTINCT Salary\n",
    "FROM Employee\n",
    "ORDER BY Salary DESC\n",
    "LIMIT 1 OFFSET n-1;\n"
   ]
  }
