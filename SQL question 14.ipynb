   "metadata": {},
   "outputs": [],
   "source": [
    "SELECT DISTINCT column_name\n",
    "FROM student\n",
    "ORDER BY column_name DESC\n",
    "LIMIT 1 OFFSET 1;\n"
   ]
  }
